{
  "swagger": "2.0",
  "info": {
    "description": "Get all files uploaded to a specific K2 Cloud smartobject",
    "version": "1.0",
    "title": "K2 Cloud - Upload File"
  },
  "host": "J17OWCY.onk2.com",
  "basePath": "/api/odatav4/v4/",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "basicAuth": {
      "type": "basic"
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ],
  "x-ntx-render-version": 2,
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/smoDocGenDemoOutputs": {
      "get": {
        "tags": [
          "List Documents"
        ],
        "summary": "List all documents",
        "operationId": "listItems",
        "responses": {
          "200": {
            "description": "Successful Response",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "ID": {
                    "type": "integer",
                    "description": "Item ID"
                  },
                  "OutputFile@odata.mediaReadLink": {
                    "type": "string",
                    "description": "Attached document"
                  }
                }
              }
            },
            "examples": {
              "application/json": {
                "data": [
                  {
                    "ID": 1,
                    "OutputFile@odata.mediaReadLink": "https://j17owcy.onk2.com/api/odatav4/resources/smoDocGenDemoOutput(1)/OutputFile"
                  },
                  {
                    "ID": 2,
                    "OutputFile@odata.mediaReadLink": "https://j17owcy.onk2.com/api/odatav4/resources/smoDocGenDemoOutput(2)/OutputFile"
                  }
                ]
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Create Document"
        ],
        "summary": "Create an item",
        "operationId": "createItem",
        "parameters": [
          {
            "name": "method",
            "in": "query",
            "required": false,
            "default": "Create",
            "type": "string",
            "x-example": "Create"
          }
        ],
        "responses": {
          "200": {
            "description": "Item created",
            "schema": {
              "type": "object",
              "properties": {
                "ID": {
                  "type": "integer",
                  "description": "Item ID"
                },
                "OutputFile@odata.mediaReadLink": {
                  "type": "string",
                  "description": "Attached document"
                }
              }
            },
            "examples": {
              "application/json": {
                "data": [
                  {
                    "ID": 1,
                    "OutputFile@odata.mediaReadLink": "https://j17owcy.onk2.com/api/odatav4/resources/smoDocGenDemoOutput(1)/OutputFile"
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
}