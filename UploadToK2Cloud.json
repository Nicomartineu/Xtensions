{
  "swagger": "2.0",
  "info": {
    "description": "Get all files uploaded to a specific K2 Cloud smartobject",
    "version": "1.0",
    "title": "K2 Cloud - Upload File"
  },
  "host": "J17OWCY.onk2.com",
  "basePath": "/api/odatav4/v4/",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "basicAuth": {
      "type": "basic"
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ],
  "x-ntx-render-version": 2,
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/smoDocGenDemoOutputs": {
      "get": {
        "tags": [
          "Documents"
        ],
        "summary": "List all documents",
        "parameters": [
          {
            "name": "ID",
            "in": "query",
            "required": false,
            "type": "integer",
            "x-example": 1
          },
          {
            "name": "OutputFile@odata.mediaReadLink",
            "in": "query",
            "required": false,
            "type": "string",
            "x-example": "https://j17owcy.onk2.com/api/odatav4/resources/smoDocGenDemoOutput(1)/OutputFile"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "examples": {
              "application/json": {
                "data": [
                  {
                    "ID": 1,
                    "OutputFile@odata.mediaReadLink": "https://j17owcy.onk2.com/api/odatav4/resources/smoDocGenDemoOutput(1)/OutputFile"
                  }
                ]
              }
            }
          }
        }
      }
    },
    "x-summary": "Uploaded Files"
  }
}