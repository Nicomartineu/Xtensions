{
  "swagger": "2.0",
  "info": {
    "description": "Get all files uploaded to a specific K2 Cloud smartobject",
    "version": "1.0",
    "title": "K2 Cloud - Upload File"
  },
  "host": "J17OWCY.onk2.com",
  "basePath": "/api/odatav4/v4/",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "basicAuth": {
      "type": "basic"
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ],
  "x-ntx-render-version": 2,
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/smoDocGenDemoOutputs": {
      "post": {
        "tags": [
          "Create Document"
        ],
        "summary": "Create an item",
        "operationId": "createItem",
        "parameters": [
          {
            "name": "method",
            "description": "Method to execute",
            "in": "query",
            "required": false,
            "default": "Create",
            "type": "string",
            "x-example": "Create",
            "x-ntx-visibility": "internal"
          },
          {
            "name": "body",
            "in": "body",
            "x-ntx-visibility":"internal",
            "schema": {
              "type": "object",
              "properties": {
                "Status": {
                  "type": "string",
                  "x-ntx-visibility":"internal",
                  "default": "Created"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Item created",
            "schema": {
              "type": "object",
              "properties": {
                "ID": {
                  "type": "integer",
                  "description": "Item ID"
                },
                "OutputFile@odata.mediaReadLink": {
                  "type": "string",
                  "description": "Attached document"
                }
              }
            }
          }
        }
      }
    }
  }
}